{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "quotes": 0,
    "strict": 0,
    "curly": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0
  }
}
